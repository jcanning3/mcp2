<p id="notice"><%= notice %></p>

<h1>Team Musicians</h1>

<table>
  <thead>
    <tr>
      <th>Team</th>
      <th>Musician</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @team_musicians.each do |team_musician| %>
      <tr>
        <td><%= team_musician.team %></td>
        <td><%= team_musician.musician %></td>
        <td><%= link_to 'Show', team_musician %></td>
        <td><%= link_to 'Edit', edit_team_musician_path(team_musician) %></td>
        <td><%= link_to 'Destroy', team_musician, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Team Musician', new_team_musician_path %>
